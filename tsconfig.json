{
  "version": "1.8.0",
  "compilerOptions": {
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "emitError": false,
    "noEmitOnError": false,
    "sourceMap": false,
    "removeComments": false,
    "target": "es5",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "experimentalAsyncFunctions": true,
    "declaration": true,
    "noExternalResolve":  false
  }
}